{
    "version": "1.0.0-*",
    "testRunner": "xunit",
    "dependencies": {
        "Microsoft.NETCore.App": "1.1.0",
        "dotnet-test-xunit": "2.2.0-preview2-build1029",
        "xunit": "2.2.0-beta2-build3300",
        "CoreCRM": {
            "target": "project"
        },
        "Moq": "4.6.38-alpha",
        "Microsoft.DotNet.InternalAbstractions": "1.0.0"
    },
    "frameworks": {
        "netcoreapp1.1": {
            "imports": [
                "dotnet5.6",
                "portable-net45+win8"
            ]
        }
    },
    "runtimes": {
        "win8-x64": {},
        "win10-x64": {}
    },
    "buildOptions": {
        "copyToOutput": {
            "include": [ "xunit.runner.json" ]
        }
    },
    "tooling": {
        "defaultNamespace": "CoreCRM.UnitTest"
    }
}
